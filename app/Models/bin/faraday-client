#! /usr/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'faradaysec==3.8.0','console_scripts','faraday-client'
__requires__ = 'faradaysec==3.8.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('faradaysec==3.8.0', 'console_scripts', 'faraday-client')()
    )
